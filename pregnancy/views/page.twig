{% extends "layout.twig" %}

{% block content %}
    <section class="contact">
	<div class="container">
        {% for post in posts %}
	<div class="contact-sec">
        	<h1 class="contact-heading text-center">{{ post.name }}</h1>
            <div class="row"> 
            <div class="col-md-12">
                <p>{{ post.post_content }}</p>
            </div>
	    </div>
	</div>
        {% endfor %}
    </div>
    </section>
{% endblock %}
